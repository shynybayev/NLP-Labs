    "sec_per_m = 60\n",
    "sec_per_h = 3600\n",
    "sec_per_d = 86400\n",
    "\n",
    "sec = int(input())\n",
    "\n",
    "d = sec / sec_per_d\n",
    "sec = sec % sec_per_d\n",
    " \n",
    "h = sec / sec_per_h\n",
    "sec = sec % sec_per_h\n",
    " \n",
    "m = sec / sec_per_m\n",
    "sec = sec %  sec_per_m\n",
    " \n",
    "print(\"%d:%02d:%02d:%02d\"%(d,h,m,sec))"
   ]
