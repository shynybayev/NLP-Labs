    "m = float(input(\"water in mm \"))\n",
    "t = float(input(\"temperature Celsius \"))\n",
    "\n",
    "h_c_w = 4.186\n",
    "elect_c = 8.9 \n",
    "j_k = 2.777**-7\n",
    "\n",
    "Q = h_c_w*m*t\n",
    "print(\"%d. joules of energy \"%Q)\n",
    "\n",
    "k_h = Q * j_k\n",
    "cost = elect_c * k_h\n",
    "\n",
    "print(\"This energy costs %.2f cents\"%cost)"
